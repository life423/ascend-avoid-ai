{
  "name": "ascend-avoid-mobile",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 5173",
    "build": "vite build",
    "preview": "vite preview --host 0.0.0.0",
    "server": "node server/index.js",
    "kill-ports": "lsof -ti:5173,8080 | xargs kill -9 2>/dev/null || true",
    "start": "npm run kill-ports && npm run server & npm run dev"
  },
  "devDependencies": {
    "vite": "^5.0.0"
  },
  "dependencies": {
    "ws": "^8.0.0"
  }
}